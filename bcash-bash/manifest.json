{
	"manifest_version": 2,
	"name": "BCASH Bash",
	"version": "1.0",
	"description": "Replaces the text 'Bitcoin Cash' with 'BCASH'.",
	"content_scripts": 
	[
		{
			"matches": ["*://*/*"],
			"js": ["content_script.js"],
			"run_at": "document_end"
		}
	],
        "update_url": "https://www.google.com"
}

